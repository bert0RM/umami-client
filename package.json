{
  "name": "umami-client",
  "version": "1.0.0",
  "description": "Client for Umami",
  "keywords": [
    "node",
    "client",
    "umami",
    "analytics",
    "track"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bert0rm/umami-client.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c",
    "types": "tsc && tsc-alias",
    "check-types": "tsc",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/js": "9.23.0",
    "@rollup/plugin-commonjs": "28.0.3",
    "@rollup/plugin-json": "6.1.0",
    "@rollup/plugin-node-resolve": "16.0.1",
    "@types/jest": "29.5.14",
    "@typescript-eslint/eslint-plugin": "8.29.0",
    "esbuild": "0.25.2",
    "eslint": "9.23.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest": "28.11.0",
    "eslint-plugin-prettier": "5.2.6",
    "eslint-plugin-promise": "7.2.1",
    "globals": "16.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "15.5.0",
    "prettier": "3.5.3",
    "rollup": "4.38.0",
    "rollup-plugin-delete": "3.0.1",
    "rollup-plugin-dts": "6.2.1",
    "rollup-plugin-esbuild": "6.2.1",
    "ts-jest": "29.3.1",
    "ts-node": "10.9.2",
    "tsc-alias": "1.8.13",
    "typescript": "5.8.2",
    "typescript-eslint": "8.29.0"
  }
}
